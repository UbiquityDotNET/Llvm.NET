{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "src/Ubiquity.NET.Llvm/Ubiquity.NET.Llvm.csproj",
                        "src/Ubiquity.NET.Llvm.JIT/Ubiquity.NET.Llvm.JIT.csproj"
                    ],
                    "src": ".."
                }
            ],
            "dest": "current/api",
            "categoryLayout": "Nested",
            "namespaceLayout": "Nested",
            "memberLayout": "SeparatePages",
            "enumSortOrder": "DeclaringOrder"
        }
    ],
    "build": {
        "xref": [
            "versions-xref.yml",
            "current/llvm-xref.yml",
            "https://learn.microsoft.com/en-us/dotnet/.xrefmap.json"
        ],
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "files": [
                    "toc.yml",
                    "index.md",
                    "ReleaseNotes.md",
                    "api/**.md",
                    "api/**.yml",
                    "articles/**.md",
                    "articles/**.yml"
                ],
                "src": "current",
                "dest": "current"
            },
            {
                "files": [
                    "Kaleidoscope/**/Kaleidoscope*.md",
                    "CodeGenWithDebugInfo/**/codegeneration.md",
                    "OrcV2VeryLazy/**/OrcV2VeryLazy.md"
                ],
                "src": "../Samples",
                "dest": "current/articles/Samples"
            }
        ],
        "resource": [
            {
                "files": [
                    "favicon.ico",
                    "DragonSharp48x48.png",
                    "DragonSharp.png"
                ],
                "src": "."
            },
            {
                "files": [
                    "/Samples/**/*.svg"
                ],
                "src": "..",
                "dest": "current/articles"
            }
        ],
        "overwrite": [
            {
                "files": [
                    "**/apidoc/**.md",
                    "current/namespaces/**.md"
                ],
                "exclude": [ "obj/**" ]
            }
        ],
        "template": [
            "default",
            "modern",
            "templates/Ubiquity"
        ],
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "postProcessors": [],
        "noLangKeyword": false,
        "globalMetadata": {
            "_appTitle": "Ubiquity.NET.Llvm",
            "_appFooter": "Copyright (C) 2017-2025, Ubiquity.NET Contributors",
            "_appLogoPath": "DragonSharp48x48.png",
            "_disableBreadcrumb": true,
            "assemblies": [
                "Ubiquity.NET.Llvm"
            ],
            "_gitContribute": {
                "repo": "https://github.com/UbiquityDotNET/Llvm.NET",
                "branch": "develop"
            },
            "_gitUrlPattern": "github"
        }
    }
}
