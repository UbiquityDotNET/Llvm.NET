{
  "metadata":
  [
    {
      "src":
      [
        {
          "files": [ "src/Llvm.Net/**.cs"  ],
          "src":".."
        }
      ],
      "dest": "api",
      "properties":
      {
          "TargetFramework": "net47"
      }
    }
  ],
  "build":
  {
    "xref": [ "llvm-xref.yml", "../tools/msdn.4.5.2/content/msdn.4.5.2.zip", "../tools/msdn.4.5.2/content/namespaces.4.5.2.zip"],
    "content":
    [
      {
        "files": [ "toc.yml",
                   "index.md",
                   "api/**.md",
                   "api/**.yml",
                   "articles/**.md",
                   "articles/**.yml",
                 ],
        "exclude": [ "obj/**" ]
      }
    ],
    "resource":
    [
      {
        "files": ["favicon.ico",
                  "DragonSharp48x48.png",
                  "DragonSharp.png",
                  "articles/**.svg"
                  ]
      }
    ],
    "overwrite":
    [
      {
        "files": [ "apidoc/**.md" ],
        "exclude": [ "obj/**" ]
      }
    ],
    "dest": "../BuildOutput/docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "noLangKeyword": false,
    "globalMetadata":
    {
      "_appTitle" : "Llvm.NET",
      "_appFooter" : "Copyright (C) 2017, Dot NET Foundation",
      "_appLogoPath": "DragonSharp48x48.png",
      "_disableBreadcrumb" : true,
      "llvmVersion": "5.0.0"
    },
    "template": ["statictoc","../tools/memberpage/content","templates/Ubiquity"]
  }
}
