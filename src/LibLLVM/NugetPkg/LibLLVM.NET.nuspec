<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata minClientVersion="4.0">
        <id>LibLLVM.NET</id>
        <version>$version$</version>
        <authors>LLVM.NET</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Extended LLVM-C API DLLs for LLVM v$llvmversion$</description>
        <developmentDependency>true</developmentDependency>
        <projectUrl>http://github.com/netmf/LLVM.NET</projectUrl>
        <licenseUrl>http://releases.llvm.org/$llvmversion$/LICENSE.TXT</licenseUrl>
        <tags>LLVM,compiler,native</tags>
    </metadata>
    <files>
        <file src="build\LibLLVM.NET.props" target="build\LibLLVM.NET.props" />
        <file src="buildMultiTargeting\LibLLVM.NET.props" target="buildMultiTargeting\LibLLVM.NET.props" />

        <file src="$buildbinoutput$$configuration$\x86\LibLLVM.pdb" target="runtimes\win-x86\native" />
        <file src="$buildbinoutput$$configuration$\x86\LibLLVM.dll" target="runtimes\win-x86\native" />

        <file src="$buildbinoutput$$configuration$\x64\LibLLVM.pdb" target="runtimes\win-x64\native" />
        <file src="$buildbinoutput$$configuration$\x64\LibLLVM.dll" target="runtimes\win-x64\native" />
    </files>
</package>
